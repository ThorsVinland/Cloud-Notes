{
  "expo": {
    "name": "SkyNotes",
    "slug": "SkyNotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SkyNotes.png",
    "scheme": "skynotes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/SkyNotes.png"
      },
      "splash": {
        "image": "./assets/images/SkyNotes.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.SkyNotes"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/SkyNotes.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SkyNotes.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}